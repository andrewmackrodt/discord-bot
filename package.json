    "@stylistic/eslint-plugin-ts": "^4.2.0",
    "@swc/core": "^1.11.22",
    "@swc/helpers": "^0.5.17",
    "@swc/jest": "^0.2.38",
    "@types/better-sqlite3": "^7.6.13",
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
