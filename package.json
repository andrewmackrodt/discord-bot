    "eslint-plugin-unused-imports": "^4.1.4",
    "fs-extra": "^11.3.1",
    "jest": "^30.0.5",
    "npm-run-all2": "^8.0.4",
    "shelljs": "^0.10.0",
    "shx": "^0.4.0",
    "strong-mock": "^9.0.1",
