    "@stylistic/eslint-plugin": "^5.2.3",
    "@swc/core": "^1.13.3",
    "@swc/helpers": "^0.5.17",
    "@swc/jest": "^0.2.39",
    "@types/better-sqlite3": "^7.6.13",
    "@types/fs-extra": "^11.0.4",
    "@types/gm": "^1.25.4",
    "@types/jest": "^30.0.0",
