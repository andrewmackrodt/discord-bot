{
  "name": "discord-bot",
  "version": "0.0.0",
  "description": "Node.js Discord Bot",
  "license": "UNLICENSED",
  "author": {
    "name": "Andrew Mackrodt",
    "email": "andrewmackrodt@gmail.com",
    "url": "https://gitlab.com/andrewmackrodt/discord-bot"
  },
  "main": "index.js",
  "scripts": {
    "build": "run-s clean build:compile",
    "build:check": "tsc --noEmit",
    "build:compile": "ts-node build",
    "check": "run-p build:check format:check lint:check license:check vulnerability:check",
    "clean": "shx rm -rf \"./out/\"",
    "coverage": "jest --collect-coverage",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "license:check": "license-checker --excludePackages \"$(echo $(npm pkg get name)@$(npm pkg get version) | sed 's/\\\"//g')\" --production --unknown --onlyAllow='Apache-2.0;BSD;CC0-1.0;ISC;MIT;MPL-2.0'",
    "lint": "eslint --fix",
    "lint:check": "eslint",
    "lint:pr": "eslint --format rdjson . | reviewdog -f=rdjson -reporter=github-pr-review",
    "lint:push": "eslint --format rdjson . | reviewdog -f=rdjson -reporter=github-check",
    "start": "ts-node index.ts",
    "test": "jest",
    "typeorm": "ts-node ./node_modules/typeorm/cli -d src/db",
    "vulnerability:check": "pnpm audit --audit-level critical --ignore-registry-errors  --ignore-unfixable --prod",
    "watch": "ts-node-dev --respawn --transpile-only index"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "better-sqlite3": "^12.2.0",
    "bufferutil": "^4.0.9",
    "cheerio": "^1.1.2",
    "discord.js": "^14.22.1",
    "dotenv": "^17.2.1",
    "glob": "^11.0.3",
    "gm": "^1.25.1",
    "node-schedule": "^2.1.1",
    "openai": "^5.16.0",
    "reflect-metadata": "^0.2.2",
    "spotify-web-api-node": "^5.0.2",
    "tslib": "^2.8.1",
    "tsyringe": "^4.10.0",
    "typeorm": "^0.3.26",
    "utf-8-validate": "^6.0.5"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.2",
    "@eslint/js": "^9.34.0",
    "@jest/types": "^30.0.5",
    "@stylistic/eslint-plugin": "^5.2.3",
    "@swc/core": "^1.13.5",
    "@swc/helpers": "^0.5.17",
    "@swc/jest": "^0.2.39",
    "@types/better-sqlite3": "^7.6.13",
    "@types/fs-extra": "^11.0.4",
    "@types/gm": "^1.25.4",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.18.0",
    "@types/node-schedule": "^2.1.8",
    "@types/shelljs": "^0.8.17",
    "@types/spotify-web-api-node": "^5.0.11",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-formatter-rdjson": "^1.0.6",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-perfectionist": "^4.15.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-unused-imports": "^4.2.0",
    "fs-extra": "^11.3.1",
    "globals": "^16.3.0",
    "jest": "^30.1.1",
    "license-checker": "^25.0.1",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "prettier-plugin-jsdoc": "^1.3.3",
    "prettier-plugin-multiline-arrays": "^4.0.3",
    "prettier-plugin-packagejson": "^2.5.19",
    "shx": "^0.4.0",
    "strong-mock": "^9.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  },
  "packageManager": "pnpm@10.15.0+sha512.486ebc259d3e999a4e8691ce03b5cac4a71cbeca39372a9b762cb500cfdf0873e2cb16abe3d951b1ee2cf012503f027b98b6584e4df22524e0c7450d9ec7aa7b",
  "engines": {
    "node": ">=22"
  }
}
