{
  "name": "discord-bot",
  "version": "0.0.0",
  "description": "Node.js Discord Bot",
  "license": "UNLICENSED",
  "author": {
    "name": "Andrew Mackrodt",
    "email": "andrewmackrodt@gmail.com",
    "url": "https://gitlab.com/andrewmackrodt/discord-bot"
  },
  "scripts": {
    "build": "npm-run-all clean build:compile",
    "build:compile": "ts-node build",
    "clean": "shx rm -rf out/",
    "coverage": "jest --collect-coverage",
    "lint": "eslint --ext ts .",
    "lint:check": "eslint --ext ts --format rdjson . | reviewdog -f=rdjson -reporter=github-check",
    "lint:fix": "eslint --ext ts --fix .",
    "lint:review": "eslint --ext ts --format rdjson . | reviewdog -f=rdjson -reporter=github-pr-review",
    "start": "ts-node index",
    "test": "jest",
    "typeorm": "ts-node ./node_modules/typeorm/cli -d src/db.ts",
    "watch": "ts-node-dev --respawn --transpile-only index"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "better-sqlite3": "^9.4.5",
    "bufferutil": "^4.0.8",
    "cheerio": "^1.0.0-rc.12",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.5",
    "glob": "^10.3.12",
    "gm": "^1.25.0",
    "node-schedule": "^2.1.1",
    "openai": "^4.32.2",
    "reflect-metadata": "^0.2.2",
    "spotify-web-api-node": "^5.0.2",
    "tslib": "^2.6.2",
    "tsyringe": "^4.8.0",
    "typeorm": "^0.3.20",
    "utf-8-validate": "^6.0.3",
    "zlib-sync": "^0.1.9"
  },
  "devDependencies": {
    "@swc/core": "^1.4.12",
    "@swc/helpers": "^0.5.8",
    "@swc/jest": "^0.2.36",
    "@types/better-sqlite3": "^7.6.9",
    "@types/cheerio": "^0.22.35",
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "@types/gm": "^1.25.4",
    "@types/jest": "^29.5.12",
    "@types/node": "~20.12.4",
    "@types/node-schedule": "^2.1.7",
    "@types/shelljs": "^0.8.15",
    "@types/spotify-web-api-node": "^5.0.11",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "~7.3.1",
    "@typescript-eslint/parser": "~7.3.1",
    "eslint": "^8.57.0",
    "eslint-formatter-rdjson": "^1.0.6",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "fs-extra": "^11.2.0",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "shelljs": "^0.8.5",
    "shx": "^0.3.4",
    "strong-mock": "^8.0.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "~5.4.4"
  }
}
