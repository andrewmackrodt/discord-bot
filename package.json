{
  "name": "discord-bot",
  "version": "0.0.0",
  "private": true,
  "description": "Node.JS Discord Bot",
  "license": "UNLICENSED",
  "author": {
    "name": "Andrew Mackrodt",
    "email": "andrewmackrodt@gmail.com",
    "url": "https://gitlab.com/andrewmackrodt/discord-bot"
  },
  "scripts": {
    "build": "npm-run-all dist build:deps",
    "build:deps": "npm ci --silent --no-progress --production --prefix=dist/",
    "dist": "npm-run-all clean dist:bundle",
    "dist:bundle": "ts-node-dev package",
    "clean": "shx rm -rf dist/",
    "lint": "eslint --ext ts .",
    "lint:check": "eslint --ext ts --format rdjson . | reviewdog -f=rdjson -reporter=github-check",
    "lint:fix": "eslint --ext ts --fix .",
    "lint:review": "eslint --ext ts --format rdjson . | reviewdog -f=rdjson -reporter=github-pr-review",
    "start": "ts-node-dev index",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli",
    "watch": "ts-node-dev --respawn index"
  },
  "dependencies": {
    "axios": "^0.25.0",
    "better-sqlite3": "^7.5.0",
    "cheerio": "^1.0.0-rc.10",
    "discord.js": "^12.5.3",
    "dotenv": "^14.3.2",
    "glob": "^7.2.0",
    "gm": "^1.23.1",
    "node-schedule": "^2.1.0",
    "reflect-metadata": "^0.1.13",
    "spotify-web-api-node": "^5.0.2",
    "tslib": "^2.3.1",
    "twitter-api-client": "^1.5.1",
    "typeorm": "^0.2.41"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.4.2",
    "@types/cheerio": "^0.22.30",
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^7.2.0",
    "@types/gm": "^1.18.11",
    "@types/node": "^14.18.9",
    "@types/node-schedule": "^1.3.2",
    "@types/shelljs": "^0.8.11",
    "@types/spotify-web-api-node": "^5.0.6",
    "@types/ws": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^8.7.0",
    "eslint-formatter-rdjson": "^1.0.5",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "fs-extra": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "shelljs": "^0.8.5",
    "shx": "^0.3.4",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.5"
  }
}
