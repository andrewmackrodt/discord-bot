{
  "name": "discord-bot",
  "version": "0.0.0",
  "private": true,
  "description": "Node.JS Discord Bot",
  "license": "UNLICENSED",
  "author": {
    "name": "Andrew Mackrodt",
    "email": "andrewmackrodt@gmail.com",
    "url": "https://gitlab.com/andrewmackrodt/discord-bot"
  },
  "scripts": {
    "build": "npm-run-all clean build:compile",
    "build:compile": "ts-node build",
    "clean": "shx rm -rf out/",
    "coverage": "jest --collect-coverage",
    "lint": "eslint --ext ts .",
    "lint:check": "eslint --ext ts --format rdjson . | reviewdog -f=rdjson -reporter=github-check",
    "lint:fix": "eslint --ext ts --fix .",
    "lint:review": "eslint --ext ts --format rdjson . | reviewdog -f=rdjson -reporter=github-pr-review",
    "start": "ts-node index",
    "test": "jest",
    "typeorm": "ts-node ./node_modules/typeorm/cli -d src/db.ts",
    "watch": "ts-node-dev --respawn --transpile-only index"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "better-sqlite3": "^8.5.1",
    "cheerio": "^1.0.0-rc.12",
    "discord.js": "^12.5.3",
    "dotenv": "^16.3.1",
    "glob": "^10.3.3",
    "gm": "^1.25.0",
    "node-schedule": "^2.1.1",
    "openai": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "spotify-web-api-node": "^5.0.2",
    "tslib": "^2.6.2",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@swc/core": "^1.3.78",
    "@swc/helpers": "^0.5.1",
    "@swc/jest": "^0.2.29",
    "@types/better-sqlite3": "^7.6.4",
    "@types/cheerio": "^0.22.31",
    "@types/fs-extra": "^11.0.1",
    "@types/glob": "^8.1.0",
    "@types/gm": "^1.25.1",
    "@types/jest": "^29.5.3",
    "@types/node": "^18.17.6",
    "@types/node-schedule": "^2.1.0",
    "@types/shelljs": "^0.8.12",
    "@types/spotify-web-api-node": "^5.0.7",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.47.0",
    "eslint-formatter-rdjson": "^1.0.6",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "fs-extra": "^11.1.1",
    "jest": "^29.6.2",
    "npm-run-all": "^4.1.5",
    "shelljs": "^0.8.5",
    "shx": "^0.3.4",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  }
}
