{
   "type": "better-sqlite3",
   "database": "./config/scratchmo.db",
   "synchronize": false,
   "logging": false,
   "entities": [
      "src/models/**/*.{ts,js}",
      "src/plugins/*/models/**/*.{ts,js}"
   ],
   "migrations": [
      "src/migrations/**/*.{ts,js}",
      "src/plugins/*/migrations/**/*.{ts,js}"
   ],
   "subscribers": [
      "src/subscribers/**/*.{ts,js}",
      "src/plugins/*/subscribers/**/*.{ts,js}"
   ],
   "cli": {
      "entitiesDir": "src/models",
      "migrationsDir": "src/migrations",
      "subscribersDir": "src/subscriber"
   }
}
